    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.6.0",
    "eslint-plugin-redux-saga": "1.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^4.0.1",
    "image-webpack-loader": "^8.1.0",
    "imports-loader": "0.8.0",
